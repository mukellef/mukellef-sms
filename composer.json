{
    "name": "mukellef/sms",
    "description": "Send sms with Php",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.2",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4" : {
            "Mukellef\\Sms\\" : "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mukellef\\Sms\\LaravelServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.14"
    }
}
